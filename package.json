{
  "name": "svelte-manage-web",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "description": "基于 Svelte 5 + TypeScript + Vite 7 + Tailwind CSS 4 构建的管理后台框架",
  "author": "zhoolg <https://github.com/zhoolg>",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/zhoolg/svelte-manage-web.git"
  },
  "keywords": [
    "svelte",
    "typescript",
    "vite",
    "tailwindcss",
    "admin",
    "framework",
    "crud",
    "dashboard"
  ],
  "scripts": {
    "dev": "vite",
    "build": "prettier --write . && vite build",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.app.json && tsgo -p tsconfig.node.json",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "create-module": "node scripts/create-module.cjs",
    "remove-module": "node scripts/remove-module.cjs"
  },
  "dependencies": {
    "@internationalized/date": "^3.10.1",
    "bits-ui": "^2.15.4",
    "d3-array": "^3.2.4",
    "d3-scale": "^4.0.2",
    "d3-shape": "^3.2.0",
    "jsencrypt": "^3.5.4",
    "layerchart": "^1.0.13",
    "lucide-svelte": "^0.563.0",
    "tailwindcss-primeui": "^0.6.1"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^6.2.4",
    "@tailwindcss/postcss": "^4.1.18",
    "@tsconfig/svelte": "^5.0.6",
    "@types/node": "^24.10.9",
    "@typescript/native-preview": "7.0.0-dev.20260127.1",
    "autoprefixer": "^10.4.23",
    "flowbite": "^4.0.1",
    "flowbite-svelte": "^1.31.0",
    "postcss": "^8.5.6",
    "prettier": "^3.8.1",
    "prettier-plugin-svelte": "^3.4.1",
    "svelte": "^5.48.5",
    "svelte-check": "^4.3.5",
    "tailwindcss": "^4.1.18",
    "vite": "^7.3.1"
  },
  "optionalDependencies": {
    "@rollup/rollup-win32-x64-msvc": "^4.57.0"
  }
}
