{
  "name": "@zhoolg/svelte-admin-framework",
  "version": "0.0.1",
  "description": "基于 Svelte 5 + TypeScript + Tailwind CSS 4 的管理后台框架",
  "type": "module",
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./components": {
      "types": "./dist/components/index.d.ts",
      "svelte": "./dist/components/index.js",
      "default": "./dist/components/index.js"
    },
    "./stores": {
      "types": "./dist/stores/index.d.ts",
      "svelte": "./dist/stores/index.js",
      "default": "./dist/stores/index.js"
    },
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "svelte": "./dist/utils/index.js",
      "default": "./dist/utils/index.js"
    },
    "./config": {
      "types": "./dist/config/index.d.ts",
      "svelte": "./dist/config/index.js",
      "default": "./dist/config/index.js"
    },
    "./locales": {
      "types": "./dist/locales/index.d.ts",
      "svelte": "./dist/locales/index.js",
      "default": "./dist/locales/index.js"
    }
  },
  "files": [
    "dist",
    "!dist/**/*.test.ts",
    "!dist/**/*.spec.ts"
  ],
  "scripts": {
    "build": "svelte-package -i src -o dist",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "prepublishOnly": "pnpm run build"
  },
  "peerDependencies": {
    "svelte": "^5.0.0",
    "bits-ui": "^2.0.0",
    "lucide-svelte": "^0.400.0"
  },
  "devDependencies": {
    "@sveltejs/package": "^2.3.7",
    "@sveltejs/vite-plugin-svelte": "^6.0.0",
    "svelte": "^5.48.5",
    "svelte-check": "^4.3.5",
    "typescript": "^5.7.3",
    "vite": "^7.0.0"
  },
  "dependencies": {
    "@internationalized/date": "^3.10.1",
    "jsencrypt": "^3.5.4"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/zhoolg/svelte-manage-web.git",
    "directory": "packages/svelte-admin-framework"
  },
  "author": "zhoolg <https://github.com/zhoolg>",
  "license": "GPL-3.0",
  "keywords": [
    "svelte",
    "svelte5",
    "admin",
    "framework",
    "crud",
    "typescript",
    "tailwindcss"
  ]
}
